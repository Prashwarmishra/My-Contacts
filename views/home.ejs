<!DOCTYPE html>
<html>
    <head>
        <title>
            <%= title %>
        </title>

    </head>
    <body>
        <h1><i class="fas fa-address-book"></i>Contacts</h1>
        <div id="contact-container">
            <ul>
                <%for (let i of contact_list){%>
                    <li id="contact">
                        <div id="details">
                            <p><%= i.name %></p>
                            <p><%= i.phone %></p>
                        </div>
                        <div>
                            <a href="">
                                <i class="fas fa-window-close"></i>
                            </a>
                        </div>
                    </li>
                <%}%>
            </ul>
        </div>
    </body>
</html>